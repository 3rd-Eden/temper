{
  "name": "temper",
  "version": "0.3.2",
  "description": "Temper compiles template for client and server side usage.",
  "main": "index.js",
  "scripts": {
    "test": "mocha $(find test -name '*.test.js')",
    "watch": "mocha --watch $(find test -name '*.test.js')",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- $(find test -name '*.test.js')",
    "test-travis": "istanbul cover node_modules/.bin/_mocha --report lcovonly -- $(find test -name '*.test.js')"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bigpipe/temper.git"
  },
  "keywords": [
    "temper",
    "templating",
    "browser",
    "compile",
    "jade",
    "html",
    "hogan.js",
    "handlebars",
    "ejs"
  ],
  "author": "Arnout Kazemier",
  "license": "MIT",
  "devDependencies": {
    "assume": "~1.4.1",
    "ejs": "~2.5.2",
    "handlebars": "~4.0.6",
    "hogan.js": "3.0.x",
    "istanbul": "~0.4.5",
    "jade": "~1.11.0",
    "mocha": "~3.1.2",
    "pre-commit": "~1.1.3",
    "react-jsx": "~1.0.0"
  },
  "dependencies": {
    "demolish": "1.0.x",
    "diagnostics": "~1.0.1",
    "tick-tock": "~1.0.0"
  }
}
